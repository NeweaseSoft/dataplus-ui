<?xml version="1.0" encoding="UTF-8"?>
<module rename-to="backstage">
    <!-- GWT stuffs -->
    <inherits name="com.github.gwtboot.starter.Starter"/>

    <!-- GWT DominoUI components -->
    <inherits name="org.dominokit.domino.ui.DominoUI"/>

    <!-- Other module inherits -->

    <!-- Theme -->
    <inherits name='com.google.gwt.user.theme.clean.Clean'/>

    <!-- Gss -->
    <set-configuration-property name="CssResource.enableGss" value="true"/>

    <!-- Specify the app entry point class -->
    <entry-point class='com.neweasesoft.dataplus.ui.backstage.client.AppEntryPoint'/>

    <!-- Specify the paths for translatable code -->
    <source path='client'/>
    <source path='shared'/>
</module>
